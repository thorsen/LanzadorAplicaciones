<?xml version = '1.0' encoding = 'UTF-8' standalone = 'yes'?>
<jazn-data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://xmlns.oracle.com/oracleas/schema/jazn-data-11_0.xsd">
  <jazn-realm default="jazn.com">
    <realm>
      <name>jazn.com</name>
    </realm>
  </jazn-realm>
  <policy-store>
    <applications>
      <application>
        <name>LanzadorAplicaciones</name>
        <app-roles>
        </app-roles>
        <resource-types>
          <resource-type>
            <name>RegionResourceType</name>
            <display-name>Web Page</display-name>
            <description>Region resource type</description>
            <matcher-class>oracle.adf.share.security.authorization.RegionPermission</matcher-class>
            <actions-delimiter>,</actions-delimiter>
            <actions>view,customize,grant,personalize</actions>
          </resource-type>
          <resource-type>
            <name>TaskFlowResourceType</name>
            <display-name>Task Flow</display-name>
            <description>Task Flow resource type</description>
            <matcher-class>oracle.adf.controller.security.TaskFlowPermission</matcher-class>
            <actions-delimiter>,</actions-delimiter>
            <actions>view,customize,grant,personalize</actions>
          </resource-type>
        </resource-types>
        <resources>
          <resource>
            <name>/untitled1.jspx</name>
            <display-name>untitled1 (No Page Definition)</display-name>
            <description>/untitled1.jspx</description>
            <type-name-ref>RegionResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/WEB-INF/AltasPeticionesTF.xml#AltasPeticionesTF</name>
            <display-name>AltasPeticionesTF(/WEB-INF)</display-name>
            <description>/WEB-INF/AltasPeticionesTF.xml#AltasPeticionesTF</description>
            <type-name-ref>TaskFlowResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/WEB-INF/cartasColorTF.xml#cartasColorTF</name>
            <display-name>cartasColorTF(/WEB-INF)</display-name>
            <description>/WEB-INF/cartasColorTF.xml#cartasColorTF</description>
            <type-name-ref>TaskFlowResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/WEB-INF/FfabccapTF.xml#FfabccapTF</name>
            <display-name>FfabccapTF(/WEB-INF)</display-name>
            <description>/WEB-INF/FfabccapTF.xml#FfabccapTF</description>
            <type-name-ref>TaskFlowResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/WEB-INF/FfabccpeTF.xml#FfabccpeTF</name>
            <display-name>FfabccpeTF(/WEB-INF)</display-name>
            <description>/WEB-INF/FfabccpeTF.xml#FfabccpeTF</description>
            <type-name-ref>TaskFlowResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/WEB-INF/tf1.xml#tf1</name>
            <display-name>tf1(/WEB-INF)</display-name>
            <description>/WEB-INF/tf1.xml#tf1</description>
            <type-name-ref>TaskFlowResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/WEB-INF/tf2.xml#tf2</name>
            <display-name>tf2(/WEB-INF)</display-name>
            <description>/WEB-INF/tf2.xml#tf2</description>
            <type-name-ref>TaskFlowResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/WEB-INF/tfPrincipal.xml#tfPrincipal</name>
            <display-name>tfPrincipal(/WEB-INF)</display-name>
            <description>/WEB-INF/tfPrincipal.xml#tfPrincipal</description>
            <type-name-ref>TaskFlowResourceType</type-name-ref>
          </resource>
          <resource>
            <name>/welcome.jspx</name>
            <display-name>welcome (No Page Definition)</display-name>
            <description>/welcome.jspx</description>
            <type-name-ref>RegionResourceType</type-name-ref>
          </resource>
        </resources>
        <permission-sets>
          <permission-set>
            <name>Cartas de Color</name>
            <member-resources>
              <member-resource>
                <resource-name>/WEB-INF/AltasPeticionesTF.xml#AltasPeticionesTF</resource-name>
                <type-name-ref>TaskFlowResourceType</type-name-ref>
                <display-name>AltasPeticionesTF(/WEB-INF)</display-name>
                <actions>view</actions>
              </member-resource>
              <member-resource>
                <resource-name>/WEB-INF/cartasColorTF.xml#cartasColorTF</resource-name>
                <type-name-ref>TaskFlowResourceType</type-name-ref>
                <display-name>cartasColorTF(/WEB-INF)</display-name>
                <actions>view</actions>
              </member-resource>
              <member-resource>
                <resource-name>/WEB-INF/FfabccapTF.xml#FfabccapTF</resource-name>
                <type-name-ref>TaskFlowResourceType</type-name-ref>
                <display-name>FfabccapTF(/WEB-INF)</display-name>
                <actions>view</actions>
              </member-resource>
              <member-resource>
                <resource-name>/WEB-INF/FfabccpeTF.xml#FfabccpeTF</resource-name>
                <type-name-ref>TaskFlowResourceType</type-name-ref>
                <display-name>FfabccpeTF(/WEB-INF)</display-name>
                <actions>view</actions>
              </member-resource>
            </member-resources>
          </permission-set>
        </permission-sets>
        <jazn-policy>
          <grant>
            <grantee>
              <principals>
                <principal>
                  <name>authenticated-role</name>
                  <class>oracle.security.jps.internal.core.principals.JpsAuthenticatedRoleImpl</class>
                </principal>
              </principals>
            </grantee>
            <permissions>
              <permission>
                <class>oracle.adf.share.security.authorization.RegionPermission</class>
                <name>es.ramondin.lanzadoraplicaciones.view.pageDefs.mainPageDef</name>
                <actions>view</actions>
              </permission>
              <permission>
                <class>oracle.adf.controller.security.TaskFlowPermission</class>
                <name>/WEB-INF/welcome.xml#welcome</name>
                <actions>view</actions>
              </permission>
              <permission>
                <class>oracle.adf.controller.security.TaskFlowPermission</class>
                <name>/WEB-INF/antiguasTF.xml#antiguasTF</name>
                <actions>view</actions>
              </permission>
            </permissions>
            <permission-set-refs>
              <permission-set-ref>
                <name>Cartas de Color</name>
              </permission-set-ref>
            </permission-set-refs>
          </grant>
          <grant>
            <grantee>
              <principals>
                <principal>
                  <name>anonymous-role</name>
                  <class>oracle.security.jps.internal.core.principals.JpsAnonymousRoleImpl</class>
                </principal>
              </principals>
            </grantee>
            <permissions>
              <permission>
                <class>oracle.adf.share.security.authorization.RegionPermission</class>
                <name>es.ramondin.lanzadoraplicaciones.view.pageDefs.loginPageDef</name>
                <actions>view</actions>
              </permission>
            </permissions>
          </grant>
        </jazn-policy>
      </application>
    </applications>
  </policy-store>
</jazn-data>
